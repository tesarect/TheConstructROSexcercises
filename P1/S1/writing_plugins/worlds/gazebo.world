<?xml version="1.0"?>
<sdf version="1.4">
  <world name="default">
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- A plugin -->
    <plugin name="a_model_plugin" filename="liba_model_plugin.so"/>
  </world>
</sdf>